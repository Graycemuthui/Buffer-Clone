<div class = "d-flex align-items-center justify-content-center">
<h1> Welcome to schedhuled tweets </h1>
</div>

<div class = "d-flex align-items-center justify-content-center logout">
  <% if @user %>
  <div class = "alert alert-success col-md-6 d-flex align-items-center" role="alert">
  Logged in as <%= @user.email %>
  </div>
  
  <%= button_to "Logout", logout_path, method: :delete, class: "btn btn-primary " %>
  <% end %>
</div>
